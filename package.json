{
    "name": "tinycarecode",
    "displayName": "tinycarecode",
    "description": "A VS Code extension that displays self-care reminders from @tinycarebot. Inspired by tiny-care-terminal.",
    "version": "0.0.1",
    "publisher": "thefifthisa",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.tinyCare"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.tinyCare",
                "title": "tinycarecode"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "twit": "^2.2.10"
    }
}
